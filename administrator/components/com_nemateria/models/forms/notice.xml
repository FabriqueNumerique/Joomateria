<?xml version="1.0" encoding="utf-8"?>
<!-- $Id: entry.xml 21 2011-03-29 17:14:01Z chdemko $ -->
<form>
	<fieldset>
		<field name="id_notice"
			type="text"
			label="JGLOBAL_FIELD_ID_LABEL"
			description="JGLOBAL_FIELD_ID_DESC"
			size="10"
			readonly="true"
			class="readonly"
                        default="0"
		/>
                <field name="published"
			type="list"
			label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC"
			class="inputbox"
			size="1"
			default="1"
		>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
		</field>
		<field name="locked"
			type="list"
			label="COM_NEMATERIA_LOCK"
			description="Rendre l'élément modifiable/non modifiable"
			class="inputbox"
			size="1"
			default="1"
		>
			<option value="1">JNO</option>
			<option value="0">JYES</option>
		</field>
                 <field name="id_collection"
			type="textarea"
			label="COM_NEMATERIA_SELECT_COLLECTION"
			description="Id de la collection"
			class="text"
			size="80"
			required="false"
		 />
		<field name="title"
			type="textarea"
			label="COM_NEMATERIA_TITLE"
			description="Titre de la collection"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="unique_identifier"
			type="textarea"
			label="COM_NEMATERIA_HEADERS_NOTICE_UNIQUE_IDENTIFIER"
			description="identifiant unique de la notice"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="alias"
			type="textarea"
			label="COM_NEMATERIA_ALIAS"
			description="wg"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="creator"
			type="textarea"
			label="COM_NEMATERIA_CREATOR"
			description="Alias de la notice"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="subject"
			type="textarea"
			label="COM_NEMATERIA_SUBJECT"
			description="Sujet"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="type"
			type="textarea"
			label="COM_NEMATERIA_TYPE"
			description="Type de document"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="description"
			type="textarea"
			label="COM_NEMATERIA_DESCRIPTION"
			description="Description"
			class="textarea"
			size="100"
			required="false"
			filter="safehtml"
		 />
                 <field name="publisher"
			type="textarea"
			label="COM_NEMATERIA_PUBLISHER"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
                 <field name="contributor"
			type="textarea"
			label="COM_NEMATERIA_CONTRIBUTOR"
			description="Contributeur ayant posté la notice"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="date"
			type="textarea"
			label="COM_NEMATERIA_DATE"
			description="Date"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="format"
			type="textarea"
			label="COM_NEMATERIA_FORMAT"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="identifier"
			type="textarea"
			label="COM_NEMATERIA_IDENTIFIER"
			description="format de la notice"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="source"
			type="textarea"
			label="COM_NEMATERIA_SOURCE"
			description="Source de la notice"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="language"
			type="textarea"
			label="COM_NEMATERIA_LANGUAGE"
			description="Langue"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="relation"
			type="editor"
            label="COM_NEMATERIA_RELATION"
			description="COM_NEMATERIA_RELATION"
            class="form_field"
            default=""
            filter="safehtml"
		 />
                  <field name="coverage"
			type="textarea"
			label="COM_NEMATERIA_COVERAGE"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="rights"
			type="textarea"
			label="COM_NEMATERIA_RIGHTS"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="datestamp"
			type="textarea"
			label="COM_NEMATERIA_DATESTAMP"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
                  <field name="metadata"
			type="textarea"
			label="COM_NEMATERIA_METADATA"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
		 />
            <field name="champs"
			type="textarea"
			label="COM_NEMATERIA_VARIOUS"
			description="wgwgr"
			class="inputbox"
			size="100"
			required="false"
			filter="safehtml"
		 />
        </fieldset>
        <fieldset>
            <field name="local_link"
			type="textarea"
			label="COM_NEMATERIA_LOCAL_LINK"
			description="wgwgr"
			class="inputbox"
            filter="safehtml"
			size="100"
			required="false"
		 />
        </fieldset>
</form>
